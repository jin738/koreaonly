    <title>@ViewBag.Title - Korea Only</title>

    <script>

        $(document).ready(function () {

            $(".SubDivs").addClass("animated");
            $(".SubDivs").addClass("growIn");

        });


        function checkit(checkboxid, valueboxid, valueboxname) {
            if ($("#" + checkboxid).is(":checked")) {
                $("#" + valueboxid).val(valueboxname + "1");
                //$("#" + checkboxid).prop('checked', true);
            } else {
                //$("#" + checkboxid).prop('checked', false);
                $("#" + valueboxid).val(valueboxname + "0");
            }

            dosearch("check it echeck");

        }

        function setEventOnlyDiv(trigerdiv, targetdiv) {

            $("#" + targetdiv).hide();

            $("#" + trigerdiv).hover(function () {

                $("#" + targetdiv).show();

            });
            $("#" + trigerdiv).mouseleave(function () {

                $("#" + targetdiv).hide();

            });

        }

    </script>

