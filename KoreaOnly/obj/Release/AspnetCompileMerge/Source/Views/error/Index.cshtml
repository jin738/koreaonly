@model string[]


@{
    ViewBag.Title = "Error";
}

<head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="~/scripts/jserror.js" type="text/javascript"></script>
    <script src="~/scripts/errorjs.js"></script>

    <link rel="shortcut icon" href="~/Images/favicon.jpg" type="image/x-icon">
    <link href="~/Content/error.css" rel="stylesheet" />
</head>

<div class="container">

    <div id="container">
        <div id="stage" class="stage">
            <div id="clouds" class="stage"></div>
        </div>

        <div id="ticket">
            <section id="ticket_left">
                <p class="text1_a">Lost in the clouds</p>
                <p class="text2_a">@Model[1]</p>
                <p class="text3_a">Error @Model[0]</p>
                <p class="text4_a">Sorry!</p>
                <p class="text5_a">From</p>
                <p class="text6_a">Somewhere</p>
                <p class="text7_a">To</p>
                <p class="text8_a">Nowhere</p>
                <p class="text9_a">Seat</p>
                <p class="text10_a">@Model[0]</p>
                <p class="text11_a">Try another flight</p>
                <nav class="text12_a">
                    <ul>
                        <li><a href="/Index/">Home</a></li>
                        <li><a href="/Index/Aboutus/">About</a></li>
                        <li><a href="/Index/privacy_policy">Privacy Policy</a></li>
                        <li><a href="/Index/termsandconditions">Terms and Conditions</a></li>
                    </ul>
                </nav>
            </section>

            <section id="ticket_right">
                <p class="text1_b">First class</p>
                <p class="text2_b">Lost in the clouds</p>
                <p class="text3_b">From</p>
                <p class="text4_b">Somewhere</p>
                <p class="text5_b">To</p>
                <p class="text6_b">Nowhere</p>
                <p class="text7_b">Seat</p>
                <p class="text8_b">@Model[0]</p>
                <p class="text9_b">1</p>
                <p class="text10_b">103076498</p>
            </section>
        </div>
    </div>

</div>


<script type="text/javascript">
    (function ($) {
        $(document).ready(function () {
            $('#clouds').pan({ fps: 40, speed: 0.7, dir: 'right', depth: 10 });
        });
    })(jQuery);
</script>